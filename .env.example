# Configurações do Banco de Dados PostgreSQL
POSTGRES_DB=esus
POSTGRES_USER=postgres
POSTGRES_PASSWORD=esus

# Configurações do e-SUS PEC
# URL para download do instalador do e-SUS
# Obtenha a versão mais recente em: https://sisaps.saude.gov.br/esus/
URL_DOWNLOAD_ESUS=https://arquivos.esusab.ufsc.br/PEC/26e603822f8adcc4/5.3.28/eSUS-AB-PEC-5.3.28-Linux64.jar

# Modo de Instalação
# Define se a instalação será de Produção (false) ou Treinamento (true)
# Modo Treinamento: usado para fins de capacitação e testes, sem dados reais
# Modo Produção: usado para ambiente de produção com dados reais
# Padrão: false (Produção)
ESUS_TRAINING_MODE=false
